{
  "name": "nuxt-aeo",
  "version": "1.1.4",
  "description": "Nuxt module for AI Engine Optimization (AEO) using Schema.org JSON-LD structured data",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "schema.org",
    "json-ld",
    "seo",
    "aeo",
    "ai-engine-optimization",
    "structured-data",
    "schema",
    "person",
    "article",
    "faq",
    "itemlist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dewdew/nuxt-aeo.git"
  },
  "author": "Yeonju Lee (Dewdew)",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "version:major": "node scripts/version-runner.cjs major",
    "version:minor": "node scripts/version-runner.cjs minor",
    "version:patch": "node scripts/version-runner.cjs patch",
    "version:clear": "node scripts/version-runner.cjs clear"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.21.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "~5.9.3",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.8"
  }
}
